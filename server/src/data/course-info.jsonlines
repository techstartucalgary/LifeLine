{"cid": 40858, "code": "COMS", "number": 617, "topic": "Representation and Identity", "description": "An investigation of various issues related to representation and identity, including but not limited to race, ethnicity, gender and sexuality.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3S-0"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"40858\">Communication and Media Studies 617</course> and Culture and Society 603 will not be allowed", "notes": null, "aka": "(formerly Communication and Culture 617)", "repeat": false, "nogpa": false}
{"cid": 40854, "code": "COMS", "number": 619, "topic": "Communications and Cultural Industries", "description": "An analysis of the governmental and social contexts which inform the current development of telecommunications, communications, cultural industries and new media in Canada. ", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3S-0"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"40854\">Communication and Media Studies 619</course> and Communications Studies 619 will not be allowed.", "notes": null, "aka": "(formerly <course cid=\"40854\">Communication and Culture 619</course>)", "repeat": false, "nogpa": false}
