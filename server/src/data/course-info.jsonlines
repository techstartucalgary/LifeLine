{"cid": 8010, "code": "FILM", "number": 591, "topic": "Capstone Seminar in Film Studies", "description": "Explores the variety of ways in which film and the technologies of motion pictures connect with social life with reference to a specific topic. Completion of a major project that will integrate understanding of film theory, history and genres.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["2-3"], "time_length": null, "prereq": "Admission to the BA in Film Studies and 78 units, of which, 6 units must be taken from <course cid=\"7996\">Film 321</course>, <course cid=\"7997\">323</course>, <course cid=\"7998\">331</course>, or <course cid=\"7999\">333</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 42849, "code": "FILM", "number": 595, "topic": "Honours Seminar", "description": "Individual research and seminar activities (e.g., critiques, presentations, and peer reviews of drafts) to produce a proposal and a paper to be used as the basis for the honours thesis.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["2S-0"], "time_length": null, "prereq": "Admission to BA Honours Film Studies.", "coreq": null, "antireq": "Credit for <course cid=\"42849\">Film 595</course> and Film 590 or Communication and Media Studies 590 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 42850, "code": "FILM", "number": 597, "topic": "Honours Thesis and Seminar", "description": "Supervised individual research and preparation of an honours thesis. Incorporating material from the Film 595 final paper, to complete the honours thesis. Seminar activities include progress reports, peer reviews, and support for poster presentations and oral exams.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["1S-0"], "time_length": null, "prereq": "<course cid=\"42849\">Film 595</course> and admission to BA Honours in Film Studies.", "coreq": null, "antireq": "Credit for <course cid=\"42850\">Film 597</course> and Film 590 or Communication and Media Studies 590 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 7741, "code": "ENGL", "number": 203, "topic": "Introductory Seminar", "description": "An intensive introduction to the field of English emphasizing critical reading and writing as well as discussion. Expectations for writing and research will be higher than in other junior English courses. Includes the study of at least one text by an Indigenous author.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"7741\">English 203</course> and 201 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 48216, "code": "ENGL", "number": 212, "topic": "Critical Writing and Indigenous Literatures", "description": "An examination of how literature as a cultural practice responds to and reflects its social context through an analysis of the works of Indigenous writers. Focuses on critical writing and reading skills, alongside the importance of story-telling and oral tradition in Indigenous cultural traditions.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "Admission to a Faculty Indigenous Pathway Program.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 43745, "code": "ENGL", "number": 251, "topic": "Literature and Society", "description": "An examination of the relationship of literature to the world around us, with instruction in close reading and critical writing. Considers how literature as a cultural practice responds to and reflects its social context. Includes the study of at least one text by an Indigenous author.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0", "2-1T", "1.5-1.5T"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"43745\">English 251</course> and 201 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 43746, "code": "ENGL", "number": 253, "topic": "Studies in Genre", "description": "A study of the conventions of a genre, with emphasis on close reading and critical writing skills. Includes the study of at least one text by an Indigenous author.", "sub_topics": {"01": "Short Fiction", "02": "Poetry", "03": "Novel", "04": "Drama", "05": "Non-fiction"}, "units": 3.0, "credits": null, "hours": ["3-0", "2-1T", "1.5-1.5T"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"43746\">English 253</course>.02 and 351 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 7748, "code": "ENGL", "number": 265, "topic": "Introductory Creative Writing", "description": "Instruction in the rudiments of craft for poetry, scripts, and fiction, emphasizing the centrality of reading and revision to literary composition.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["1.5-1.5S"], "time_length": null, "prereq": null, "coreq": null, "antireq": null, "notes": "Does not count toward the Embedded Certificate in Creative Writing.", "aka": null, "repeat": false, "nogpa": false}
{"cid": 7749, "code": "ENGL", "number": 302, "topic": "Introduction to Contemporary Theory", "description": "An examination of the claims and assumptions of a range of contemporary critical practices, such as formalism, structuralism, deconstruction, feminism and gender studies, new historicism, psychoanalytic criticism, and cultural and ideological critique. Includes sustained engagement with original theoretical texts.", "sub_topics": null, "units": 6.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "6 units of courses labelled English.", "coreq": null, "antireq": null, "notes": "Students with prior credit in <course cid=\"7749\">English 302</course> will not be allowed to take 303.", "aka": null, "repeat": false, "nogpa": false}
{"cid": 41990, "code": "ENGL", "number": 303, "topic": "Theories for Reading", "description": "An examination of contemporary critical and cultural theories and their practical applications to reading. Students will learn models of critical reading and gain experience applying them to a variety of literary and cultural texts.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": null, "coreq": null, "antireq": "Credit for <course cid=\"41990\">English 303</course> and <course cid=\"7749\">302</course> will not be allowed.", "notes": "Students with prior credit in <course cid=\"7749\">English 302</course> will not be allowed to take 303. Students are permitted to take <course cid=\"41990\">English 303</course> prior to 302.", "aka": null, "repeat": false, "nogpa": false}
