{"cid": 8699, "code": "SOCI", "number": 405, "topic": "Topics in Canadian Social Structure", "description": "A macro-level examination of Canadian social structure including a socio-developmental analysis of selected Canadian institutions such as bilingualism, regionalism, multiculturalism, multinational corporations, and ethnic stratification. Critical examination of sociological models for the study of Canadian society.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8679\">Sociology 313</course>, <course cid=\"8680\">315</course>, <course cid=\"8683\">331</course>, <course cid=\"8684\">333</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": true, "nogpa": false}
{"cid": 8700, "code": "SOCI", "number": 407, "topic": "Sociology of the Body", "description": "Considers aspects of the lived body and the body as object of social construction, modification, and regulation.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8679\">Sociology 313</course>, <course cid=\"8680\">315</course>, <course cid=\"8683\">331</course>, <course cid=\"8684\">333</course>; and 3 units from 303, 307, 321, 373, 375, 399.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8701, "code": "SOCI", "number": 409, "topic": "Social Determinants of Health", "description": "An advanced study of health in a social context. Examines the social determinants of health and illness outcomes along with their implications for public policies conducive to better health.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8679\">Sociology 313</course>, <course cid=\"8680\">315</course>, <course cid=\"8681\">321</course>, <course cid=\"8683\">331</course>, <course cid=\"8684\">333</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
