{"cid": 10030, "code": "ECON", "number": 499, "topic": "Intermediate Topics in Economics", "description": "Examination of a particular topic. Examples include advanced game theory, ethics and economics, water resources, and selected topics from other fields of economics.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9995\">Economics 357</course>, <course cid=\"9996\">359</course> and <course cid=\"44503\">395</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": true, "nogpa": false}
