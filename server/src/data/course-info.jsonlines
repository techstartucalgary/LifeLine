{"cid": 8326, "code": "SOWK", "number": 301, "topic": "Generalist Practice in Context Portfolio Project", "description": "Individual preparation of a self-directed portfolio as an expression of the student's own understanding of generalist social work practice in relation to the local context. The student's prior learning experiences will be incorporated in the development of the project.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["1S-2T"], "time_length": null, "prereq": null, "coreq": "Prerequisite or Corequisite: <course cid=\"8325\">Social Work 300</course>.", "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
