{"cid": 8685, "code": "SOCI", "number": 341, "topic": "Social Psychology", "description": "A wide range of interdependent individual and social topics that influence our thinking, feeling, and behaviour. Topics may include social cognition, perception, social experience, group behaviour, social interaction, and emotions.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": "Credit for <course cid=\"8685\">Sociology 341</course> and <course cid=\"8758\">Psychology 345</course> will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8686, "code": "SOCI", "number": 345, "topic": "Mass Communication", "description": "A critical, theoretically-informed look at what it means to live in a society in which the forms and content of mass media/mass communication - and our uses of them - shape our lives in significant ways.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8687, "code": "SOCI", "number": 353, "topic": "Urban Sociology", "description": "Discusses the impact of urbanization on social relationships by analyzing city living in historical and global perspective but also through local issues such as density and neighbourhoods.  Provides a sociological perspective on issues that cities are facing today.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8688, "code": "SOCI", "number": 355, "topic": "Population and Society", "description": "Introduction to social demography - the study of population structure (size, composition and distribution) and processes (fertility, mortality and migration). Focuses on both empirical and theoretical relationships between demography, social change and structured inequality.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8689, "code": "SOCI", "number": 365, "topic": "Social Stratification", "description": "Introduction to the sociological study of social inequalities and stratification and the major theoretical explanations of social inequalities.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8690, "code": "SOCI", "number": 371, "topic": "Sociology of Families", "description": "A sociological examination of family life in all its multiple and changing forms. Topics may include key transitions and common practices in family life; gender, sexuality and family life, and family life in cross-cultural context.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8691, "code": "SOCI", "number": 373, "topic": "Sociology of Aging", "description": "Social and institutional factors associated with aging in modern society.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8692, "code": "SOCI", "number": 375, "topic": "Sociology of Ethnicity and Racialization", "description": "Introduction to the forms of ethnic and racial dynamics in selected cases around the world. Such concepts as identity, ethnicity, race, racialization, racism, assimilation, integration, pluralism, colonialism, post-colonialism, transnationalism and diasporas are examined in theoretical and cross-national contexts.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8693, "code": "SOCI", "number": 377, "topic": "Sociology of Religion", "description": "An introduction to the theories and concepts utilized by sociologists to interpret religious behaviour and the organization of religion.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8694, "code": "SOCI", "number": 393, "topic": "Sociology of Work", "description": "A sociological perspective for gaining a better understanding of the meaning and nature of work. Provides an overview of core concepts and current topics fundamental to understanding work in society.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 8695, "code": "SOCI", "number": 399, "topic": "Sociology of Sport", "description": "Organized around the theme of social problems in sport, examines sports-related behaviour, consumption, and sponsorship. A variety of approaches shows how sport is socially constructed and embodies different meanings for different groups of people.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"8672\">Sociology 201</course>.", "coreq": null, "antireq": "Credit for <course cid=\"8695\">Sociology 399</course> and <course cid=\"40518\">Kinesiology 244</course> will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
