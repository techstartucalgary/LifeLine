{"cid": 9985, "code": "ECON", "number": 323, "topic": "Natural Gas Markets", "description": "Operation of the natural gas industry in North America. Economics of exploration, development, production, marketing and transportation of natural gas. Impact of government regulations and deregulation.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9976\">Economics 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 44495, "code": "ECON", "number": 328, "topic": "Petroleum Economics", "description": "Crude oil production and markets in North America and globally. Refining economics and markets for refined products. Government policy including royalty regimes, trade quotas, transport infrastructure, conservation and the environment.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9976\">Economics 201</course>.", "coreq": null, "antireq": "Credit for <course cid=\"44495\">Economics 328</course> and 325 or 327 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9988, "code": "ECON", "number": 329, "topic": "Electricity Markets", "description": "Economic analysis of the deregulated electric power industry including the pricing of power, energy and capacity, power supply and demand, market structure, market architecture, and the design and testing of market rules.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9976\">Economics 201</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
