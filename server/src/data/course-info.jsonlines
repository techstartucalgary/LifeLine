{"cid": 9435, "code": "ARKY", "number": 625, "topic": "Hunter-Gatherer Adaptations", "description": "Intensive study of contemporary and prehistoric hunter-gatherer social and economic adaptations.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3S-0"], "time_length": null, "prereq": null, "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
