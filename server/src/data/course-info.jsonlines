{"cid": 9415, "code": "ARKY", "number": 555, "topic": "Human Osteology", "description": "Identification and interpretation of human skeletal and dental remains. Emphasis is on functional anatomy and reconstruction of prehistoric lifeways.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-2"], "time_length": null, "prereq": "3 units from <course cid=\"39791\">Anthropology 309</course>, <course cid=\"41952\">350</course>, <course cid=\"9386\">Archaeology 417</course>.", "coreq": null, "antireq": "Credit for <course cid=\"9415\">Archaeology 555</course> and <course cid=\"9429\">613</course> will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 44482, "code": "ARKY", "number": 590, "topic": "Honours Thesis", "description": "Research project under the direction of a member of the Department.", "sub_topics": null, "units": 6.0, "credits": null, "hours": null, "time_length": ["12-36 hours"], "prereq": "Admission to BA or BSc Archaeology Honours program and consent of the Department.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9417, "code": "ARKY", "number": 591, "topic": "Landscape Archaeology", "description": "Human perceptions and uses of the biophysical and cultural environment. The emphasis is on the act of humanizing the environment by naming places, identifying resources, establishing paths, and modifying the natural landscape thereby creating a tradition of land use that can be accessed archaeologically.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9398\">Archaeology 451</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9418, "code": "ARKY", "number": 593, "topic": "Household Archaeology", "description": "Human perceptions and uses of the built environment, particularly residential architecture. The emphasis is on the structure and symbolism associated with the spatial arrangements of objects, activities, and social interactions.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "<course cid=\"9398\">Archaeology 451</course>.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9419, "code": "ARKY", "number": 595, "topic": "Problems in Palaeopathology and Palaeonutrition", "description": "Patterns of disease in prehistoric human populations with consideration to the interaction of health and nutrition. Techniques for determining disease and nutrition from prehistoric remains are covered.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-0"], "time_length": null, "prereq": "9 units <course cid=\"8488\">Anthropology 201</course>, <course cid=\"41952\">350</course>, <course cid=\"9415\">Archaeology 555</course> and admission to the Archaeology or Anthropology major.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9421, "code": "ARKY", "number": 597, "topic": "Independent Study", "description": "An independent study course for Archaeology majors under the supervision of a faculty member.", "sub_topics": null, "units": 3.0, "credits": null, "hours": null, "time_length": ["12-36 hours"], "prereq": "Admission to the BA or BSc Archaeology and consent of the Department.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": true, "nogpa": false}
{"cid": 9424, "code": "ARKY", "number": 601, "topic": "Theoretical Foundations", "description": "The philosophy of science, the history of anthropological theory, and a survey of contemporary theoretical approaches in anthropology. Throughout, the relevance to and connections with the subdisciplines of archaeology and biological anthropology will be emphasized.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3S-0"], "time_length": null, "prereq": "Consent of the Department.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": false, "nogpa": false}
{"cid": 9425, "code": "ARKY", "number": 603, "topic": "Seminar on Special Topics", "description": "Intensive study of special problems of particular interest to Archaeology Department graduate students. Subject matter for any particular year to be left to the discretion of the Department.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3S-0"], "time_length": null, "prereq": "Consent of the Department.", "coreq": null, "antireq": null, "notes": null, "aka": null, "repeat": true, "nogpa": false}
{"cid": 9429, "code": "ARKY", "number": 613, "topic": "Analysis of Human Skeletal Remains", "description": "Methods of analyzing human remains from archaeological contexts with emphasis on identification and description. Lecture, lab and weekly seminar directed to Archaeology graduate students who have not had a previous course in human osteology.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-1T-2"], "time_length": null, "prereq": "Consent of the Department.", "coreq": null, "antireq": "Credit for <course cid=\"9429\">Archaeology 613</course> and either <course cid=\"9415\">555</course> or <course cid=\"9425\">603</course>.07 will not be allowed.", "notes": null, "aka": null, "repeat": false, "nogpa": false}
