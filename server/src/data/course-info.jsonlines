{"cid": 43702, "code": "SCIE", "number": 431, "topic": "Principles of Hydroelectric Energy", "description": "An introduction to the scientific fundamentals of hydroelectric energy production. Includes an examination of the technologies and potential for developing hydroelectricity, along with the environmental, societal, and economic issues surrounding its development.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-2T"], "time_length": null, "prereq": "3 units from <course cid=\"43062\">Science 317</course>, <course cid=\"40091\">Geology 353</course> or Engineering Energy and Environment 355.", "coreq": null, "antireq": null, "notes": null, "aka": "(formerly Science 531)", "repeat": false, "nogpa": false}
{"cid": 43703, "code": "SCIE", "number": 433, "topic": "Principles of Geothermal Energy", "description": "The scientific principles governing geothermal energy development for both low and high temperature systems, and related technical, environmental and socioeconomic issues.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-2T"], "time_length": null, "prereq": "3 units from <course cid=\"43062\">Science 317</course>, <course cid=\"40091\">Geology 353</course> or Engineering Energy and Environment 355.", "coreq": null, "antireq": null, "notes": null, "aka": "(formerly Science 533)", "repeat": false, "nogpa": false}
