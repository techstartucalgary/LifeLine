{"cid": 42518, "code": "ENPE", "number": 623, "topic": "Reservoir Analysis and Description", "description": "Data analysis and integration for reservoir modelling and simulation.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-1"], "time_length": null, "prereq": null, "coreq": null, "antireq": null, "notes": "This course does not count towards the degree requirements of MSc and PhD students.", "aka": null, "repeat": false, "nogpa": false}
{"cid": 42519, "code": "ENPE", "number": 624, "topic": "Enhanced Oil Recovery", "description": "Introduction to water and polymer flooding, miscible displacements and surfactant flooding with focus on case studies.", "sub_topics": null, "units": 3.0, "credits": null, "hours": ["3-1"], "time_length": null, "prereq": null, "coreq": null, "antireq": null, "notes": "This course does not count towards the degree requirements of MSc and PhD students.", "aka": null, "repeat": false, "nogpa": false}
